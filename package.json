{
  "name": "algory",
  "version": "1.7.7",
  "description": "An Auto-Curried, Auto-Memoized general purpose package composed of algorithms and data structures for JavaScript, TypeScript and Node.js",
  "main": "lib/index.js",
  "types": "lib",
  "scripts": {
    "test": "jest",
    "testw": "jest --watch",
    "testc": "jest --coverage",
    "build": "tsc",
    "watch": "tsc -w",
    "debug": "node --debug-brk --inspect ./node_modules/jest/bin/jest -i",
    "lint": "eslintrc.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryaghan-mutum/algory.git"
  },
  "bugs": {
    "url": "https://github.com/aryaghan-mutum/algory/issues"
  },
  "keywords": [
    "computer-science",
    "cs",
    "algorithms",
    "data-structures",
    "javascript-algorithms",
    "typescript-algorithms",
    "sorting",
    "search",
    "graph",
    "tree",
    "machine-learning",
    "math",
    "shapes",
    "curry",
    "memoize"
  ],
  "author": "Anurag Muthyam <anu.drumcoder@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/aryaghan-mutum/algory#readme",
  "devDependencies": {
    "@types/jest": "^26.0.8",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.22",
    "@types/ramda": "^0.27.14",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-chai-friendly": "^0.6.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jsdoc": "^29.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "^26.2.2",
    "lint-staged": "^10.2.11",
    "mocha": "^7.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "ramda": "^0.27.1",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.17.8",
    "typescript": "^3.9.3",
    "typescript-json-schema": "^0.42.0"
  },
  "dependencies": {
    "curry": "^1.2.0",
    "fast-memoize": "^2.5.2"
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.+(ts|tsx|js)",
      "**/?(*.)+(spec|test).+(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
