{
  "name": "algory",
  "version": "1.0.0",
  "description": "a library composed of algorithms and data structures for JavaScript, TypeScript and Node.js",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aryaghan-mutum/algory.git"
  },
  "keywords": [],
  "author": "Anurag Muthyam <anu.drumcoder@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aryaghan-mutum/algory/issues"
  },
  "homepage": "https://github.com/aryaghan-mutum/algory#readme",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "jest": "^26.0.1",
    "prettier": "^2.0.5",
    "ts-jest": "^26.0.0",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {},
  "files": [
    "lib/**/*"
  ]
}
